{
  "version": "0.19.1",
  "packages": ["components/*", "css", "utils/*"],
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/dist/**",
    "**/test/**",
    "**/.*rc",
    "**/.*rc.js",
    "**/*.config.js",
    "**/*.md"
  ],
  "command": {
    "publish": {
      "allowBranch": "master"
    }
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "registry": "https://registry.npmjs.org/"
}
