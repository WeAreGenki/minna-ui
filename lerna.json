{
  "version": "0.1.0",
  "packages": [
    "components/*",
    "css",
    "utils/*"
  ],
  "ignoreChanges": [
    "**/dist/**",
    "**/test/**",
    "**/.*rc",
    "**/.*rc.js",
    "**/*.config.js",
    "**/*.md"
  ],
  "command": {
    "publish": {
      "allowBranch": "master"
    }
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "registry": "https://registry.npmjs.org/"
}
